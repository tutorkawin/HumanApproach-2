class HelloWorld {
    public static void main(String[] args) {
        int n=5;
        for(int i=1;i<=n;i++)
        {
        	for(int k=n-i;k>0;k--) //row
        	{
        		System.out.print(".");
        	}
        	for(int j=1;j<=i;j++) //column
        	{
        		System.out.print("*");
        	}
        	System.out.println();
        }
        
    }
}
